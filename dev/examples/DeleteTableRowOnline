    Document: requestDocument,
    TablePath: ToStringPointer("tables/1"),
    Index: ToInt32Pointer(int32(0)),
    Optionals: deleteRequestOptions,
}
_, _ = wordsApi.DeleteTableRowOnline(ctx, deleteRequest)